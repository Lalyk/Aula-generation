programa
{
	
	funcao inicio()
	{
		inteiro numero = 233
		
		
		faca{

				para(numero; numero <=456; numero +=5)
				{

					escreva(numero, "\n" )

					
					enquanto(numero >=300 e numero <=400)
					{
						para(numero = numero + 3; numero <=400; numero +=3)
						{
							escreva(numero, "\n" )
						}
					}
				}


				
			
		}enquanto(numero >=233 e numero <=456)
	}
}
